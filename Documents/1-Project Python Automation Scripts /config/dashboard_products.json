{
  "data_source": "data/product_data.csv",
  "data_source_type": "csv",
  "title": "Product Analytics Dashboard",
  "description": "Comprehensive product performance metrics",
  "layout": "two-column",
  "theme": "plotly_dark",
  "charts": [
    {
      "type": "histogram",
      "title": "Price Distribution",
      "data_column_x": "price",
      "color_column": "category"
    },
    {
      "type": "box",
      "title": "Rating Distribution by Category",
      "data_column_x": "category",
      "data_column_y": "rating",
      "color_column": "category"
    },
    {
      "type": "area",
      "title": "Cumulative Revenue",
      "data_column_x": "launch_date",
      "data_column_y": "revenue",
      "aggregation": "sum"
    }
  ],
  "filters": ["category", "brand"],
  "show_data_table": true,
  "rows_per_page": 15,
  "color_palette": [
    "#FF6B6B", "#4ECDC4", "#45B7D1", "#FFA07A", "#98D8C8",
    "#F7DC6F", "#BB8FCE", "#85C1E2", "#F8B739", "#52B788"
  ],
  "enable_export": true,
  "auto_refresh": true,
  "refresh_interval": 600,
  "output_folder": "./dashboards",
  "output_filename": "product_dashboard.html"
}
