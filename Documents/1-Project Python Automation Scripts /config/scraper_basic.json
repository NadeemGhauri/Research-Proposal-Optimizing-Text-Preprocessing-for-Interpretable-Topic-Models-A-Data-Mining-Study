{
  "urls": [
    "https://example.com"
  ],
  "use_selenium": false,
  "css_selectors": {
    "title": "h1",
    "paragraphs": "p",
    "heading": "h2"
  },
  "output_formats": ["json", "csv"],
  "output_folder": "./scraped_data",
  "base_filename": "basic_scrape",
  "min_delay": 2.0,
  "max_delay": 4.0,
  "requests_per_minute": 30,
  "respect_robots_txt": true,
  "rotate_user_agents": true,
  "timeout": 30,
  "verify_ssl": true
}
